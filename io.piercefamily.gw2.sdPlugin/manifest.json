{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
  "UUID": "io.piercefamily.gw2",
  "Name": "$Name",
  "Version": "0.1.0.0",
  "Author": "piercefamily",
  "Description": "$Description",
  "SupportURL": "https://github.com/brandonpierce/streamdeck-gw2/issues",
  "Category": "$Category",
  "CategoryIcon": "imgs/category-icon",
  "Icon": "imgs/plugin-icon",
  "CodePath": "bin/plugin.js",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.7"
  },
  "OS": [
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Nodejs": {
    "Version": "20",
    "Debug": "disabled"
  },
